<?xml version='1.0' encoding='utf-8'?>
<widget id="com.evothings.cordovaibeacon" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>iBeacon</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="com.unarin.cordova.beacon" spec="/home/acquaint/Desktop/cordova-plugin-ibeacon-3.x" />
    <plugin name="cordova-plugin-local-notification" spec="/home/acquaint/Desktop/cordova-plugin-local-notifications-master" />
    <engine name="android" spec="^7.0.0" />
</widget>
